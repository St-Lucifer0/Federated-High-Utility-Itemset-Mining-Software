# Environment Configuration for HUI Federated Learning Backend

# Server Configuration
PORT=8002
NODE_ENV=development

# Database Configuration
DATABASE_PATH=./hui_system.db

# Logging Configuration
LOG_LEVEL=info

# Python Integration
PYTHON_PATH=python

# Security Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# File Upload Limits
MAX_FILE_SIZE=50mb
MAX_JSON_SIZE=50mb
